
@{
    ViewData["Title"] = "Import data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<ol class="breadcrumb">
    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
</ol>

<h3>@ViewData["Title"]</h3>
<p>
    The system will create users, objectives and Key results if not already existing in the system.
</p>
<form method="post" enctype="multipart/form-data" asp-controller="Import" asp-action="Upload">
    <div class="form-group">
        <label for="file">Upload Excel file containing Email, Objective and Key Result</label>
        <input type="file" class="form-control-file" name="files" id="file">
    </div>
    <button type="submit" class="btn btn-primary">Upload</button>
</form>
